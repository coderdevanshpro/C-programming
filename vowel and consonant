#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    scanf("%c",&ch);
   int isalpha(ch);
        if(ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U'||ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
        
         printf("Vowel.");
        
    else if(isalpha(ch)==0)
    
        printf("Not an alphabet.");
    
        else
        
        printf("Consonant.");
        
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
